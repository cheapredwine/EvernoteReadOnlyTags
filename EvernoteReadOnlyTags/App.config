<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- change this to production value -->
    <!-- production value will differ; will be provided when you create the auth token -->
    <add key="URL" value="https://sandbox.evernote.com/shard/s1/notestore"/>
    <!-- change this to something unique to you-->
    <add key="contentClass" value="AUniqueValueForYou.readonly"/>
    <!-- this is the tag text that indicates a note should be set to R/O-->
    <add key="readOnlyTag" value="ReadOnly"/>
    <!-- this is your private auth token from Evernote-->
    <!-- for production: https://www.evernote.com/api/DeveloperToken.action -->
    <!-- for Sandbox: https://sandbox.evernote.com/api/DeveloperToken.action -->
    <add key="authToken" value="" />
  </appSettings>
</configuration>